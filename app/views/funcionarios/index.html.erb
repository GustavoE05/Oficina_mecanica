<div class="design">
<p style="color: green"><%= notice %></p>

<h1>Funcionarios</h1>

<div id="funcionarios">
  <% @funcionarios.each do |funcionario| %>
    <%= render funcionario %>
    <button type="submit">
      <%= link_to "Show this funcionario", funcionario %>
    </button>
  <% end %>
</div>

<button type="submit">
<%= link_to "New funcionario", new_funcionario_path %>
</button>
</div>
